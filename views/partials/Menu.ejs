<h1>Menu</h1>
<div>
    <table>
        <thead>
            <tr>
                <% headers.map((header) => { %> 
                    <th><%= header%></th>
                <%}) %>
            </tr>
        </thead>
        <tbody>
                <% rows.map((row) => { %>
                    <tr><% headers.map(header => { %>
                        <td><%= row[header] %></td>
                    <% }) %> </tr>
                <% }) %>
        </tbody>
    </table>
</div>